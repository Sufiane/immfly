// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db/dev.db"
}

model Admins {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Products {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  price       Int
}

model Users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  password String
}
